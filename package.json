{
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@prisma/client": "^6.17.1",
    "next": "15.5.6",
    "next-auth": "5.0.0-beta.29",
    "next-safe-action": "^8.0.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
