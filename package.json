    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "next-auth": "5.0.0-beta.29",
    "next-safe-action": "^8.0.11",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "resend": "^6.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "ts-pattern": "^5.8.0",
    "wtf_wikipedia": "^10.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv-cli": "^10.0.0",
    "happy-dom": "^20.0.7",
    "husky": "^9.1.7",
    "prisma": "^6.17.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "name": "bignight.party",
  "private": true,
  "scripts": {
    "build": "next build --turbopack",
    "check-types": "tsc --noEmit",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:validate": "dotenv -e .env.local -- prisma validate",
    "dev": "NODE_ENV=development next dev --turbopack",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "lint": "pnpm check-types && biome check --write",
    "prepare": "husky",
    "spice-train": "tsx scripts/spice-train.ts",
    "stack:down": "docker compose down",
    "stack:restart": "docker compose restart",
    "stack:up": "docker compose up -d",
    "start": "next start",
    "test": "dotenv -e .env.test -- vitest",
    "test:coverage": "dotenv -e .env.test -- vitest run --coverage",
    "test:models": "dotenv -e .env.test -- vitest run src/lib/models",
    "test:run": "dotenv -e .env.test -- vitest run",
    "test:services": "dotenv -e .env.test -- vitest run src/lib/services",
    "test:ui": "dotenv -e .env.test -- vitest --ui"
  },
  "version": "0.1.0"
}
